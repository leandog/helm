<Page x:Class="Radiator.BrowserPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:radiator="clr-namespace:Radiator"
      mc:Ignorable="d" 
      d:DesignWidth="1920" d:DesignHeight="1080" 
      Title="BrowserPage" Focusable="True">
    <Page.DataContext>
        <radiator:BrowserViewModel/>
    </Page.DataContext>
    <WebBrowser x:Name="Browser" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
</Page>
